[user]
	email = talyalev.kirill@gmail.com
	name = BroccoliPower
[core]
	autocrlf = input
	safecrlf = warn
	quotepath = off
	editor = vim
[alias]
  history = log --graph
  ch = checkout
  co = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  
